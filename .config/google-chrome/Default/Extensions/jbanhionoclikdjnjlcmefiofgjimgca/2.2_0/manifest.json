{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup/info.html"
   },
   "content_scripts": [ {
      "js": [ "remove-banner.js" ],
      "matches": [ "https://*.twitter.com/*" ]
   } ],
   "description": "Forces your browser to use old twitter.",
   "icons": {
      "128": "128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "GoodTwitter",
   "permissions": [ "webRequest", "webRequestBlocking", "*://*.twitter.com/", "browsingData", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.2"
}
