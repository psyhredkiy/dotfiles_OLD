{
   "background": {
      "persistent": false,
      "scripts": [ "background/background.js" ]
   },
   "browser_action": {
      "default_popup": "popup/popup.html",
      "default_title": "De-Mainstream"
   },
   "content_scripts": [ {
      "js": [ "youtube/youtube.js" ],
      "matches": [ "https://*.youtube.com/*" ]
   } ],
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "Fixes the YouTube™ algorithm to remove mainstream media results from searches.",
   "homepage_url": "https://www.demainstream.com",
   "icons": {
      "128": "img/icons/icon@8x.png",
      "16": "img/icons/icon@1x.png",
      "32": "img/icons/icon@2x.png",
      "48": "img/icons/icon@3x.png"
   },
   "incognito": "spanning",
   "key": "",
   "manifest_version": 2,
   "name": "De-Mainstream YouTube™",
   "offline_enabled": true,
   "permissions": [ "https://*.youtube.com/", "contextMenus" ],
   "short_name": "De-Mainstream",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.5",
   "version_name": "1.0.5"
}
